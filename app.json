{
  "expo": {
    "name": "OSRS Tracker",
    "description": "Personal xp gain tracker for OSRS videogame.",
    "slug": "OSRSHiscoreMobile",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios", 
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/myIcon.png",
    "splash": {
      "image": "./assets/mySplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rynzy.osrshiscoremobile"
    }
  }
}